{
  "dashboard": {
    "id": null,
    "title": "Fraud Detection Overview",
    "tags": ["fraud", "detection", "overview"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Transaction Processing Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(fraud_detection_transactions_processed_total[5m])",
            "refId": "A",
            "legendFormat": "TPS"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Fraud Detection Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "(rate(fraud_detection_decisions_total{decision=\"block\"}[5m]) / rate(fraud_detection_decisions_total[5m])) * 100",
            "refId": "A",
            "legendFormat": "Fraud Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(fraud_detection_inference_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(fraud_detection_inference_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(fraud_detection_inference_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (s)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Model Performance Metrics",
        "type": "table",
        "targets": [
          {
            "expr": "fraud_detection_model_accuracy",
            "refId": "A",
            "format": "table"
          },
          {
            "expr": "fraud_detection_model_precision",
            "refId": "B",
            "format": "table"
          },
          {
            "expr": "fraud_detection_model_recall",
            "refId": "C",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Risk Level Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "rate(fraud_detection_risk_level_total[5m]) by (risk_level)",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "fraud_detection_deployment_info",
          "titleFormat": "Deployment",
          "textFormat": "Version: {{version}}"
        }
      ]
    }
  }
}
